generator client {
  provider = "prisma-client-js"
  output   = ".generated/client-app"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL_APP")
}

model User {
  id        Int     @id @default(autoincrement())
  email     String
  name      String
  surname   String
  index_umk Int     @unique
  isAdmin   Boolean
}
